@model PracticeMvc.Models.EmployeeUser;


<form asp-controller="Login" asp-action="Register" method="post">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        <div class="col-md-3 mb-3">
        <label asp-for="Username"></label>
        <input asp-for="Username" class="form-control" />
        </div>
        <span asp-validation-for="Username" class="text-danger"></span>
    </div>
    <div class="form-group">
        <div class="col-md-3 mb-3">
        <label asp-for="PasswordHash">Password</label>
        <input asp-for="PasswordHash" class="form-control" type="password" />
        </div>
        <span asp-validation-for="PasswordHash" class="text-danger"></span>
    </div>
    <div>
       
    <button type="submit" class="btn btn-primary">Register</button>
       
    <a class="btn btn-primary" asp-controller="Login" asp-action="Login">Login</a>
       
    </div>
</form>
